{
    "name": "xeact",
    "version": "0.0.13",
    "repository": {
        "type": "git",
        "url": "https://github.com/pengzhanlee/xeact"
    },
    "keywords": [
        "react",
        "Web Components"
    ],
    "author": "Pengzhan Lee <pengzhan.lee@gmail.com>",
    "license": "Apache License 2.0",
    "main": "src/index.js",
    "module": "es/index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack --watch",
        "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib --ignore *.test.js",
        "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore *.test.js",
        "build:umd": "cross-env NODE_ENV=development webpack",
        "build:min": "cross-env NODE_ENV=production webpack",
        "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:min",
        "clean": "rimraf lib dist es coverage",
        "prepublish": "npm run clean && npm run build",
        "test": "jest",
        "ci": "npm run build && jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
    },
    "files": [
        "dist",
        "lib",
        "src",
        "es"
    ],
    "dependencies": {
        "@webcomponents/webcomponentsjs": "^1.0.13",
        "shortid": "^2.2.8"
    },
    "peerDependencies": {
        "react": "^16.0.0 || >=16.0.0",
        "react-dom": "^16.0.0 || >=16.0.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^20.0.3",
        "babel-loader": "^7.1.2",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-react-display-name": "^6.25.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-latest": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "coveralls": "^2.13.1",
        "cross-env": "^5.0.5",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.5.0",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-react": "^7.3.0",
        "jest": "^20.0.4",
        "path": "^0.12.7",
        "raf": "^3.4.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "rimraf": "^2.6.1",
        "webpack": "^3.5.5"
    },
    "jest": {
        "globals": {},
        "collectCoverageFrom": [
            "src/**/*.js"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "rootDir": "",
        "setupFiles": [
            "raf/polyfill",
            "./test/enzymeAdapter.js"
        ],
        "verbose": true,
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "transform": {
            "^.+\\.js?$": "babel-jest"
        }
    }
}
